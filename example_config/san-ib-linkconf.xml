<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE IBCONFIG SYSTEM "ibconfig.dtd">

<fabric name="LLNL IB SAN" speed="QDR" width="4x">
	
<!--
Define a chassis position/name mapping
The actual internal links of this chassis are defined in a file <model>.xml
Defining properties for these links will have to be done individually after the
chassis definition.  This is simply a way to easily define all the links
internal to a large chassis.
link properties are accepted for the entire chassis; ie
	<chassis name="Mellanox SAN chassis" model="Mellanox_IS5200" speed="SDR" width"1x">
	<chassis name="Mellanox SAN chassis" model="Mellanox_IS5200">
	</chassis>

	<chassis name="Mellanox SAN chassis" model="Mellanox_IS5200">
		<node position="L01">Mellanox IS5200 L01</node>
		<node position="L02">Mellanox IS5200 L02</node>
		<node position="L03">Mellanox IS5200 L03</node>
		<node position="L04">Mellanox IS5200 L04</node>
		<node position="L05">Mellanox IS5200 L05</node>
		<node position="L06">Mellanox IS5200 L06</node>
		<node position="L07">Mellanox IS5200 L07</node>
		<node position="L08">Mellanox IS5200 L08</node>
		<node position="L09">Mellanox IS5200 L09</node>
		<node position="L10">Mellanox IS5200 L10</node>
		<node position="L11">Mellanox IS5200 L11</node>
		<node position="L12">Mellanox IS5200 L12</node>
		<node position="S01">Mellanox IS5200 S01</node>
		<node position="S02">Mellanox IS5200 S02</node>
		<node position="S03">Mellanox IS5200 S03</node>
		<node position="S04">Mellanox IS5200 S04</node>
		<node position="S05">Mellanox IS5200 S05</node>
		<node position="S06">Mellanox IS5200 S06</node>
	</chassis>
-->
	
	<edgelist name="Mellanox IS5200 L12">
		<port num="9"> <r_port>1</r_port><r_node>ocflicsi</r_node></port>
	</edgelist>

